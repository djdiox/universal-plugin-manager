{
  "compilerOptions": {
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "module": "commonjs",
    "target": "es5",
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    }
  }
}
